msgctxt "error:account.bank.statement.move.line:"
msgid ""
"Account \"%(account)s\" in statement line \"%(line)s\" is the same as the "
"one configured as credit or debit on journal \"%(journal)s\"."
msgstr ""
"Cuenta \"%(account)s\" en línea de extracto \"%(line)s\" es igual al "
"configurado como crédito o débito en el diario\"%(journal)s\"."

msgctxt "error:account.bank.statement.move.line:"
msgid "Amount should be a positive or negative value."
msgstr "El importe debería ser un valor positivo o negativo."

msgctxt "error:account.bank.statement.move.line:"
msgid "Please provide debit and credit account on statement journal \"%s\"."
msgstr ""
"Porfavor configure la cuenta de  débito y de crédito en el diario del "
"extracto \"%s\"."

msgctxt "error:account.bank.statement.move.line:"
msgid ""
"The credit or debit account of Journal \"%s\" is not checked as \"Bank "
"Conciliation\"."
msgstr ""
"La cuenta de crédito o la de débito del diario \"%s\" no está marcada como "
"\"Conciliación bancaria\"."

